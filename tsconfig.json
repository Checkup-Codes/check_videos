{
    "compilerOptions": {
      "allowJs": true,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "jsx": "preserve",
      "strict": true,
      "isolatedModules": true,
      "target": "ESNext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "resources/js/*"
        ]
      }
    },
    "include": [
      "resources/js/**/*.ts",
      "resources/js/**/*.d.ts",
      "resources/js/**/*.vue",
      "resources/js/Store/index.ts",
      "resources/js/Store/Modules/ActiveMenu.ts"
    ]
  }
  